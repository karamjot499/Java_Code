public class Student {
    private String name;
    private int id;
    private double grade;
    private Student next;

    public Student(String name, int id, double grade) {
        this.name = name;
        this.id = id;
        this.grade = grade;
        this.next = null;
    }

    // Getters and setters for name, id, and grade

    public Student getNext() {
        return next;
    }

    public void setNext(Student next) {
        this.next = next;
    }
}

public class StudentLinkedList {
    private Student head;

    public StudentLinkedList() {
        this.head = null;
    }

    public boolean isEmpty() {
        return head == null;
    }

    public void addStudent(Student newStudent) {
        if (isEmpty()) {
            head = newStudent;
        } else {
            Student current = head;
            while (current.getNext() != null) {
                current = current.getNext();
            }
            current.setNext(newStudent);
        }
    }

    // Other methods for searching, removing, displaying, etc.

    public static void main(String[] args) {
        StudentLinkedList list = new StudentLinkedList();

        // Create student objects and add them to the linked list
        Student student1 = new Student("John Doe", 1, 90.5);
        Student student2 = new Student("Jane Smith", 2, 85.0);
        Student student3 = new Student("Mike Johnson", 3, 92.3);

        list.addStudent(student1);
        list.addStudent(student2);
        list.addStudent(student3);

        // Perform operations on the linked list as needed
        // For example, display the list
        Student current = list.head;
        while (current != null) {
            System.out.println("Name: " + current.getName() + ", ID: " + current.getId() + ", Grade: " + current.getGrade());
            current = current.getNext();
        }
    }
}
